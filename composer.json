{
    "name": "vendor_name/psr7",
    "description": "description",
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "anorlov",
            "email": "eaglecrazy@mail.ru"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4.14",
        "ext-pdo": "^7.1",
        "aura/router": "^3.1",
        "dasprid/container-interop-doctrine": "^1.1",
        "doctrine/migrations": "^1.8",
        "monolog/monolog": "^1.23",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.0",
        "symfony/console": "^4.1",
        "twig/twig": "^2.4",
        "zendframework/zend-config-aggregator": "^1.0",
        "zendframework/zend-diactoros": "^1.7",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-stratigility": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Framework\\": "src/Framework",
            "App\\": "src/App",
            "Infrastructure\\": "src/Infrastructure",
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "ext-json": "*",
        "doctrine/data-fixtures": "^1.3",
        "filp/whoops": "^2.2",
        "fzaninotto/faker": "^1.8",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "serve": "php -S 0.0.0.0:8080 -t public",
        "dev": "ln -sr config/Autoload/development.local.php.dist config/Autoload/development.local.php",
        "prod": "rm config/Autoload/zdevelopment.local.php"
    }
}
